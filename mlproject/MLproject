name: ap3

entry_points:
  benchmarks:
    parameters:
      tier:
        type: int
        default: 1
      nepochs:
        type: int
        default: 5
      mconfig:
        type: string
        default: --nocache
    command: >
      python3 -u benchmarks_sw.py
          --tier {tier}
          --batch 512
          --epochs {nepochs}
          --model rnn
          {mconfig}
  run_main:
    parameters:
      epochs:
        type: int
        default: 5
    command: >
      python run_main.py
          --epochs {epochs}
 
